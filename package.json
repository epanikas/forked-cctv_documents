{
  "name": "nodoku-demo-page",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development && next dev -p 3001",
    "dev-inspect": "cross-env NODE_ENV=development && cross-env NODE_OPTIONS='--inspect' next dev -p 3001",
    "dev-debug": "cross-env NODE_ENV=development NODE_OPTIONS='--inspect' && next dev -p 3001",
    "build": "shx rm -rf ./.next && next build --debug",
    "vercel-upgrade-build": "npm upgrade && npm ls --depth=2 && next build --debug",
    "build-analyze": "cross-env ANALYZE=true  next build --debug",
    "start": "env-cmd next start -p 3001",
    "lint": "next lint",
    "gen-component-resolver": "nodoku-gen-component-resolver",
    "gen-skin-schema": "nodoku-gen-skin-schema",
    "postcss": "postcss -o  ./output.css ./src/app/globals.css"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@next/bundle-analyzer": "^15.1.2",
    "dompurify": "^3",
    "eslint": "^8.56.0",
    "jsdom": "^25.0.1",
    "next": "^15",
    "node-html-parser": "^6.1.13",
    "nodoku-core": "^0.3",
    "nodoku-components": "^0.3",
    "nodoku-icons": "^0.2",
    "nodoku-flowbite": "^0.3",
    "nodoku-mambaui": "^0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint-config-next": "^15.0.3",
    "purgecss": "^7.0.2",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.19.0",
    "typescript": "^5"
  }
}
